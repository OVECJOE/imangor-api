[flake8]
max-line-length = 120
extend-ignore = E203, W503
exclude =
    .git,
    .venv,
    __pycache__,
    build,
    dist,
    *.pyc,
    *.pyo,
    *.pyd,
    .Python,
    env,
    pip-log.txt,
    pip-delete-this-directory.txt,
    .tox,
    .coverage,
    .coverage.*,
    .cache,
    nosetests.xml,
    coverage.xml,
    *.cover,
    *.log,
    .pytest_cache,
    .mypy_cache,
    .hypothesis,
    .idea,
    .vscode,
    *.egg-info,
    .installed.cfg,
    *.egg
per-file-ignores =
    __init__.py: F401
max-complexity = 10